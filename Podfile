# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'HamHamaIOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for HamHamaIOS
pod 'FacebookCore'
pod 'FacebookLogin'
pod 'GoogleSignIn'
pod 'Google'
pod 'lottie-ios'
pod 'Alamofire'
pod 'AlamofireImage'
pod 'Floaty'

  target 'HamHamaIOSTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'HamHamaIOSUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
